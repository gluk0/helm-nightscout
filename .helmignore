# Patterns to ignore when building packages.
# This supports shell glob matching, relative path matching, and
# negation (prefixed with !). Only one pattern per line.
.DS_Store

# Common VCS dirs
.git/
.gitignore
.bzr/
.bzrignore
.hg/
.hgignore
.svn/

# Common backup files
*.swp
*.bak
*.tmp
*.orig
*~

# Various IDEs
.project
.idea/
*.tmproj
.vscode/

# CI/CD files
.github/
ci/
.gitlab-ci.yml
.travis.yml

# Documentation source (not the generated README.md)
README.md.gotmpl
CONTRIBUTING.md
CHANGELOG.md

# Testing files
ct.yaml
*_test.yaml
test-*.yaml

# OS files
Thumbs.db
